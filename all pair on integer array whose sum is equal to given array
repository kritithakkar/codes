#include <iostream>
#include<stdlib.h>
using namespace std;

int main(int argc,char *argv[])
{
    int sum=atoi(argv[1]);
int n=atoi(argv[2]);
int arr[n];
for(int i=3;i<argc;i++)
{
    arr[i-3]=atoi(argv[i]);
}
for(int i=0;i<n;i++)
{
for(int j=i+1;j<n;j++)
{
    sum=(arr[i]+arr[j]);
    if(sum==n)
    {
        cout<<arr[i]<<" "<<arr[j]<<endl;
    }
}
}
    return 0;
}
