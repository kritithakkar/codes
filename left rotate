#include<stdio.h>
#include<stdlib.h>
int main(int argc,char *argv[])
{
    int n=atoi(argv[1]);
    int m=atoi(argv[2]);
    int arr[argc];
    for(int i=3;i<argc;i++)
    {
        arr[i-3]=atoi(argv[i]);
    }
    int j;
    for(int i=0;i<m;i++)
    {
        int temp=arr[0];
        for(j=0;j<n-1;j++)
        {
            arr[j]=arr[j+1];
        }
        arr[j]=temp;
    }
    for(int i=0;i<n;i++)
    {
        
        printf("%d ",arr[i]);
    }
    return 0;
} or for(int i=0;i<n;i++)
{
printf("%d ",arr[(i+m)%n]);
}
