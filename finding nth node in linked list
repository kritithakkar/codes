#include<stdlib.h>
#include<iostream>
using namespace std;
struct node{
    int data;
    struct node* next;
};
void push(struct node** headref,int c)
{
    struct node* head=(struct node*)malloc(sizeof(struct node));
    head->data=c;
    head->next=(*headref);
    (*headref)=head;
}
void print(struct node* head,int n)
{
    int count=0;
    while(head!=NULL)
    {
        count++;

        if(count==n)
        cout<<head->data;
head=head->next;
}
}
int main(int argc,char* argv[])
{
    int n=atoi(argv[1]);
    int arr[30],c=0;
    for(int i=2;i<argc;i++)
    {
        arr[i-2]=atoi(argv[i]);
        c++;
    }
    struct node* head=NULL;
    for(int i=c-1;i>=0;i--)
    {
        push(&head,arr[i]);
    }
   
    print(head,n);
}
