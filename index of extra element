#include<iostream>
using namespace std;
int print(int arr1[],int arr2[],int n)
{
for(int i=0;i<n-1;i++)
    {
       if(arr1[i]!=arr2[i])
            return i;
    }
    return n-1;
}
int main()
{
    int n;
    cin>>n;
    int arr1[n],arr2[n];
    for(int i=0;i<n;i++)
    {
      cin>>arr1[i];
    }
    for(int i=0;i<n-1;i++)
    {
        cin>>arr2[i];
    }
cout<<print(arr1,arr2,n);
    return 0;
}
