#include <iostream>
#include<string.h>
#include<stack>
using namespace std;
bool isOperand(char c)
{
    return(c>='a' && c<='z' || c>='A' && c<='Z');
}

void pretinfix(string s)
{
    int l=s.length();
    std::stack<string> st;
    for(int i=0;i<l;i++)
    {
        if(isOperand(s[i]))
        {
            st.push(string(1,s[i]));
        }
        else
        {
            string op1=st.top();st.pop();
            string op2=st.top();
        st.pop();
        string temp='('+op2+s[i]+op1+')';
        st.push(temp);
        }
    }
    cout<<st.top();
}

int main(int argc,char *argv[])
{
    string pre_exp =argv[1];
    pretinfix(pre_exp);
    return 0;
}
