
#include <iostream>

using namespace std;
int binary(int arr[],int l,int r,int k)
{
    if(r>=l)
    {
        int mid=l+(r-l)/2;
        if(arr[mid]==k)
        return mid;
        if(arr[mid]>k)
        return binary(arr,l,r-1,k);
        return binary(arr,l+1,r,k);
    }
    return -1;
}
int main()
{
  int n;
  cin>>n;
  int arr[n];
  for(int i=0;i<n;i++)
  {
      cin>>arr[i];
  }
  int k;
  cin>>k;
  int r=binary(arr,0,n-1,k);
(r==-1)?cout<<"elemet not present":cout<<"element is present at"<<r;
    return 0;
}
