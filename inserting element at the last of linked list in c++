#include<iostream>
#include<stdlib.h>
using namespace std;
struct Node
{
    int data;
    Node *next;
};
void push(Node *head,int val)
{
    Node *temp;
    temp=(Node*)malloc(sizeof(Node));
    temp->data=val;
temp->next=NULL;
   Node *n=NULL;
while(head!=NULL)
{
    n=head;
    head=head->next;
}
n->next=temp;
}
void print(Node *head)
{
    while(head!=NULL)
    {
    cout<<head->data<<" ";
    head=head->next;
    }
}
int main(int argc,char *argv[])
{
    Node *head=NULL;
    for(int i=argc-2;i>=1;i--)
    {
        Node *temp;
        temp=(Node*)malloc(sizeof(Node));
        temp->data=atoi(argv[i]);
        temp->next=head;
        head=temp;
    }
    int x=atoi(argv[argc-1]);
    push(head,x);
    print(head);
    return 0;
}
