#include<iostream>
#include<stdlib.h>
using namespace std;
void fun(int arr[],int n)
{
    for(int i=0;i<n;i++)
    {
        int min=-1;
        for(int j=i+1;j<n;j++)
        {
            if(arr[i]<arr[j])
            {
                min=arr[j];
                break;
            }
        }
        cout<<arr[i]<<" "<<min;
        cout<<endl;
    }
}
int main(int argc,char *argv[])
{
    int arr[argc],n=argc-1,k=0;
    for(int i=1;i<argc;i++)
    {
        arr[k++]=atoi(argv[i]);
    }
    fun(arr,n);
    return 0;
}
