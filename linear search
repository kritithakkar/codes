#include <iostream>
using namespace std;
int linear(int n,int arr[],int k)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]==k)
        return i;
    }
    return -1;
}
int main()
{
  int n;
  cin>>n;
  int arr[n];
  for(int i=0;i<n;i++)
  {
      cin>>arr[i];
  }
  int k;
  cin>>k;
  int r=linear(n,arr,k);
(r==-1)?cout<<"elemet not present":cout<<"element is present at"<<r;
    return 0;
}
