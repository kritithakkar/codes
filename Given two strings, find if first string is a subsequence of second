#include<iostream>
#include<cstring>
#include<stdlib.h>
#include<string.h>
using namespace std;
bool sub(char *a,char *b,int l,int m)
{
    if(l==0)
    return true;
    if(m==0)
    return false;
    if(a[l-1]==b[m-1])
    return sub(a,b,l-1,m-1);
    return sub(a,b,l,m-1);
}
int main(int argc,char *argv[])
{
char *a=argv[1];
char *b=argv[2];
int l=strlen(a);
int m=strlen(b);
if(sub(a,b,l,m))
cout<<"yes";
else
cout<<"no";
    return 0;
}
