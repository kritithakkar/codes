#include<iostream>
#include<stdlib.h>
using namespace std;
struct Node
{
    int data;
    Node *next;
};
Node *getNode(int x)
{
    Node *temp;
    temp=(Node*)malloc(sizeof(Node));
    temp->data=x;
    temp->next=NULL;
    return temp;
}
void insert(Node** head,int x)
{
    if(*head==NULL)
    *head=getNode(x);
    else
    {
        Node *temp=getNode(x);
        Node *ptr=*head;
        int len=0;
        while(ptr!=NULL)
        {
           len++;
           ptr=ptr->next;
        }
        int count=((len%2)==0)?(len/2):(len+1)/2;
        ptr=*head;
        while(count-->1)
        {
            ptr=ptr->next;
        }
        temp->next=ptr->next;
        ptr->next=temp;
    }
}
void display(Node* head) 
{ 
    while (head != NULL) { 
        cout << head->data << " "; 
        head = head->next; 
    } 
} 
int main(int argc,char *argv[])
{
    Node *head=NULL;
    for(int i=argc-2;i>=1;i--)
    {
        Node *temp;
        temp=(Node*)malloc(sizeof (Node));
        temp->data=atoi(argv[i]);
        temp->next=head;
        head=temp;
    }
    int x=atoi(argv[argc-1]);
    insert(&head,x);
    display(head);
    return 0;
}
