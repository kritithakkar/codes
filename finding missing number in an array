#include <iostream>
#include<stdlib.h>
using namespace std;

int main(int argc,char *argv[])
{
int n=atoi(argv[1]);
int arr[n];
for(int i=2;i<argc;i++)
{
    arr[i-2]=atoi(argv[i]);
}
int sum=0,sum1=0,sum3=0;
for(int i=1;i<=n;i++)
{
    sum+=i;
}
cout<<"sum"<<sum<<endl;
for(int i=0;i<n-1;i++)
{
    sum1+=arr[i];
}
cout<<sum1<<endl;
if(sum1!=sum)
{
    sum3=(sum-sum1);
}
cout<<sum3;
    return 0;
}
